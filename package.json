{
  "name": "kitty-cards-game",
  "type": "module",
  "scripts": {
    "start": "node dist/server/app.js",
    "dev": "nodemon",
    "dev:client": "vite",
    "dev:server": "cross-env NODE_ENV=development tsx watch server/app.ts",
    "dev:full": "concurrently \"npm run dev:client\" \"npm run dev:server\"",
    "build": "tsc",
    "build:server": "tsc -p tsconfig.server.json",
    "test": "vitest"
  },
  "dependencies": {
    "@colyseus/core": "^0.15.0",
    "@colyseus/monitor": "^0.15.0",
    "@colyseus/schema": "^2.0.0",
    "@colyseus/ws-transport": "^0.15.0",
    "colyseus.js": "^0.15.26",
    "cors": "^2.8.5",
    "express": "^4.18.3",
    "express-rate-limit": "^7.1.5",
    "phaser": "^3.88.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router-dom": "^7.1.5"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/node": "^20.11.24",
    "@vitejs/plugin-react-swc": "^3.8.0",
    "autoprefixer": "^10.4.17",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "nodemon": "^3.1.0",
    "postcss": "^8.4.35",
    "postcss-import": "^16.1.0",
    "postcss-nesting": "^13.0.1",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.3.3",
    "vite-tsconfig-paths": "^5.1.4",
    "vitest": "^3.0.5"
